    .blook{
      border: 1px solid rgba(0, 0, 0, 0.1);
      height: 380px;
    }
    .blook1{
      height: 40px;
      background: rgba(245,245,245);
    }
    .blook1 span{
      line-height: 40px;
      float: left;
      padding-left: 20px;
      font-size: 20px;
    }
    .blook2 ul{
      float: left;
      margin: 20px;
      position: relative;
    }
    .blook2 ul li{
      line-height: 42px;
      position: absolute;
      left: 0%;
    }
    .blook2 ul span{
